{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "watch": false,
    "outDir": "out",
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "./test/",
    "./dist/"
  ]
}
